{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build", "^prebuild"],
			"outputs": ["dist/**"]
		},
		"android": {
			"dependsOn": ["^android"],
			"outputs": ["android/**"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		}
	}
}
